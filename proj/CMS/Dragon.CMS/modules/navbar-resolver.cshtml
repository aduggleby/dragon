@using Dragon.CMS.CMS.Controllers
@model Dragon.CMS.CMS.Controllers.NavigationItems
@foreach (var sub in Model.Items)
{
    if (sub is NavigationItems)
    {
        @Html.Partial("navbar-items", sub as NavigationItems);
    }
    else
    {
        @Html.Partial("navbar-item", sub as NavigationItem);
    }
}